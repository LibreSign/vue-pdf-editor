{
  "name": "@libresign/vue-pdf-editor",
  "description": "vue2 pdf editor component",
  "version": "1.1.1",
  "author": "LibreCode",
  "private": false,
  "main": "lib/vue-pdf-editor.umd.js",
  "keywords": [
    "pdf",
    "pdf editor",
    "editor"
  ],
  "scripts": {
    "start": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "build:lib": "vue-cli-service build --target lib --name vue-pdf-editor --dest lib src/index.js"
  },
  "dependencies": {
    "blob-stream-i2d": "^1.0.0",
    "core-js": "^3.8.3",
    "downloadjs": "^1.4.7",
    "open": "^8.4.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "2.4.456",
    "pdfkit": "^0.14.0",
    "vue-material-design-icons": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.33",
    "postcss-load-config": "^4.0.1",
    "tailwindcss": "^3.2.1",
    "terser-webpack-plugin": "^5.3.9",
    "vue": "^2.7.16",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
