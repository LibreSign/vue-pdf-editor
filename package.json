{
  "name": "@libresign/vue-pdf-editor",
  "description": "vue2 pdf editor component",
  "version": "1.0.0",
  "author": "Librecode",
  "private": false,
  "main": "dist/vue-pdf-editor.umd.js",
  "module": "dist/vue-pdf-editor.esm.js",
  "unpkg": "dist/vue-pdf-editor.min.js",
  "keywords": [
    "pdf",
    "pdf editor",
    "editor"
  ],
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-pdf-editor.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-pdf-editor.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/vue-pdf-editor.min.js",
    "lint": "eslint packages/",
    "lint:fix": "eslint --fix packages/"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@iconify/vue2": "^2.1.0",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "11.1.0",
    "core-js": "^3.8.3",
    "downloadjs": "^1.4.7",
    "open": "^8.4.0",
    "pdf-lib": "^1.4.0",
    "pdfjs-dist": "2.4.456",
    "rollup": "1.20.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "4.1",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.2.1",
    "terser-webpack-plugin": "^5.3.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
